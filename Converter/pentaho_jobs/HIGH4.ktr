<?xml version="1.0" encoding="UTF-8"?>
<transformation>
  <info>
    <name>product_sales_and_customer_demographics</name>
    <description>This transformation merges customer, leads, and products data to analyze sales by category and customer location.</description>
    <extended_description/>
    <trans_version>1</trans_version>
    <trans_type>Normal</trans_type>
    <directory>/</directory>
    <parameters>
      <parameter>
        <name>CUSTOMERS_INPUT</name>
        <default_value>customers.csv</default_value>
        <description>Path to the customers.csv file.</description>
      </parameter>
      <parameter>
        <name>LEADS_INPUT</name>
        <default_value>leads.csv</default_value>
        <description>Path to the leads.csv file.</description>
      </parameter>
      <parameter>
        <name>PRODUCTS_INPUT</name>
        <default_value>products.csv</default_value>
        <description>Path to the products.csv file.</description>
      </parameter>
      <parameter>
        <name>OUTPUT_CSV</name>
        <default_value>sales_summary_by_country_and_category</default_value>
        <description>Base name for the output CSV file.</description>
      </parameter>
    </parameters>
  </info>
  <notepads>
    <notepad>
      <note>This transformation reads three different CSV files (customers, leads, and products), joins them together, filters the data, and then aggregates it to provide a sales summary report.</note>
      <xloc>100</xloc>
      <yloc>50</yloc>
      <width>849</width>
      <heigth>37</heigth>
      <fontname>Segoe UI</fontname>
      <fontsize>9</fontsize>
      <fontbold>N</fontbold>
      <fontitalic>N</fontitalic>
      <fontcolorred>100</fontcolorred>
      <fontcolorgreen>100</fontcolorgreen>
      <fontcolorblue>100</fontcolorblue>
      <backgroundcolorred>255</backgroundcolorred>
      <backgroundcolorgreen>205</backgroundcolorgreen>
      <backgroundcolorblue>112</backgroundcolorblue>
      <bordercolorred>100</bordercolorred>
      <bordercolorgreen>100</bordercolorgreen>
      <bordercolorblue>100</bordercolorblue>
    </notepad>
  </notepads>
  <step>
    <name>Read Customers</name>
    <type>TextFileInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <separator>,</separator>
    <enclosure>"</enclosure>
    <header>Y</header>
    <file>
      <name>${CUSTOMERS_INPUT}</name>
      <filemask/>
      <exclude_filemask/>
      <file_required>Y</file_required>
      <include_subfolders>N</include_subfolders>
    </file>
    <fields>
      <field><name>Index</name><type>Integer</type><format>#</format><currency/><decimal>.</decimal><group>,</group><length>-1</length><precision>-1</precision><trim_type>none</trim_type><repeat>N</repeat></field>
      <field><name>Customer Id</name><type>String</type><currency/><decimal/><group/><length>-1</length><precision>-1</precision><trim_type>both</trim_type><repeat>N</repeat></field>
      <field><name>First Name</name><type>String</type><currency/><decimal/><group/><length>-1</length><precision>-1</precision><trim_type>both</trim_type><repeat>N</repeat></field>
      <field><name>Last Name</name><type>String</type><currency/><decimal/><group/><length>-1</length><precision>-1</precision><trim_type>both</trim_type><repeat>N</repeat></field>
      <field><name>Company</name><type>String</type><currency/><decimal/><group/><length>-1</length><precision>-1</precision><trim_type>both</trim_type><repeat>N</repeat></field>
      <field><name>City</name><type>String</type><currency/><decimal/><group/><length>-1</length><precision>-1</precision><trim_type>both</trim_type><repeat>N</repeat></field>
      <field><name>Country</name><type>String</type><currency/><decimal/><group/><length>-1</length><precision>-1</precision><trim_type>both</trim_type><repeat>N</repeat></field>
      <field><name>Phone 1</name><type>String</type><currency/><decimal/><group/><length>-1</length><precision>-1</precision><trim_type>both</trim_type><repeat>N</repeat></field>
      <field><name>Phone 2</name><type>String</type><currency/><decimal/><group/><length>-1</length><precision>-1</precision><trim_type>both</trim_type><repeat>N</repeat></field>
      <field><name>Email</name><type>String</type><currency/><decimal/><group/><length>-1</length><precision>-1</precision><trim_type>both</trim_type><repeat>N</repeat></field>
      <field><name>Subscription Date</name><type>Date</type><format>yyyy-MM-dd</format><currency/><decimal/><group/><length>-1</length><precision>-1</precision><trim_type>none</trim_type><repeat>N</repeat></field>
      <field><name>Website</name><type>String</type><currency/><decimal/><group/><length>-1</length><precision>-1</precision><trim_type>both</trim_type><repeat>N</repeat></field>
    </fields>
  </step>
  <step>
    <name>Read Products</name>
    <type>TextFileInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <separator>,</separator>
    <enclosure>"</enclosure>
    <header>Y</header>
    <file>
      <name>${PRODUCTS_INPUT}</name>
      <filemask/>
      <exclude_filemask/>
      <file_required>Y</file_required>
      <include_subfolders>N</include_subfolders>
    </file>
    <fields>
      <field><name>Index</name><type>Integer</type><format>#</format><currency/><decimal>.</decimal><group>,</group><length>-1</length><precision>-1</precision><trim_type>none</trim_type><repeat>N</repeat></field>
      <field><name>Name</name><type>String</type><currency/><decimal/><group/><length>-1</length><precision>-1</precision><trim_type>both</trim_type><repeat>N</repeat></field>
      <field><name>Description</name><type>String</type><currency/><decimal/><group/><length>-1</length><precision>-1</precision><trim_type>both</trim_type><repeat>N</repeat></field>
      <field><name>Brand</name><type>String</type><currency/><decimal/><group/><length>-1</length><precision>-1</precision><trim_type>both</trim_type><repeat>N</repeat></field>
      <field><name>Category</name><type>String</type><currency/><decimal/><group/><length>-1</length><precision>-1</precision><trim_type>both</trim_type><repeat>N</repeat></field>
      <field><name>Price</name><type>Number</type><format>#.##</format><currency/><decimal>.</decimal><group>,</group><length>-1</length><precision>2</precision><trim_type>none</trim_type><repeat>N</repeat></field>
      <field><name>Currency</name><type>String</type><currency/><decimal/><group/><length>-1</length><precision>-1</precision><trim_type>both</trim_type><repeat>N</repeat></field>
      <field><name>Stock</name><type>Integer</type><format>#</format><currency/><decimal>.</decimal><group>,</group><length>-1</length><precision>-1</precision><trim_type>none</trim_type><repeat>N</repeat></field>
      <field><name>EAN</name><type>String</type><currency/><decimal/><group/><length>-1</length><precision>-1</precision><trim_type>both</trim_type><repeat>N</repeat></field>
      <field><name>Color</name><type>String</type><currency/><decimal/><group/><length>-1</length><precision>-1</precision><trim_type>both</trim_type><repeat>N</repeat></field>
      <field><name>Size</name><type>String</type><currency/><decimal/><group/><length>-1</length><precision>-1</precision><trim_type>both</trim_type><repeat>N</repeat></field>
      <field><name>Availability</name><type>String</type><currency/><decimal/><group/><length>-1</length><precision>-1</precision><trim_type>both</trim_type><repeat>N</repeat></field>
      <field><name>Internal ID</name><type>Integer</type><format>#</format><currency/><decimal>.</decimal><group>,</group><length>-1</length><precision>-1</precision><trim_type>none</trim_type><repeat>N</repeat></field>
    </fields>
  </step>
  <step>
    <name>Sort Customers</name>
    <type>SortRows</type>
    <description>Sorts customer data by country and company name.</description>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <directory>%%java.io.tmpdir%%</directory>
    <prefix>out</prefix>
    <sort_size>1000000</sort_size>
    <free_memory_limit>25</free_memory_limit>
    <fast_sort>N</fast_sort>
    <compress>N</compress>
    <fields>
      <field><name>Country</name><ascending>Y</ascending><case_sensitive>N</case_sensitive></field>
      <field><name>Company</name><ascending>Y</ascending><case_sensitive>N</case_sensitive></field>
    </fields>
  </step>
  <step>
    <name>Sort Products</name>
    <type>SortRows</type>
    <description>Sorts products data by category and name.</description>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <directory>%%java.io.tmpdir%%</directory>
    <prefix>out</prefix>
    <sort_size>1000000</sort_size>
    <free_memory_limit>25</free_memory_limit>
    <fast_sort>N</fast_sort>
    <compress>N</compress>
    <fields>
      <field><name>Category</name><ascending>Y</ascending><case_sensitive>N</case_sensitive></field>
      <field><name>Name</name><ascending>Y</ascending><case_sensitive>N</case_sensitive></field>
    </fields>
  </step>
  <step>
    <name>Merge Customers and Products</name>
    <type>MergeJoin</type>
    <description>This step is a logical representation of a more complex join. Due to limited common keys in the provided data, we'll assume a join on a synthetic key or a simplified join on `Country` and a derived key from a related table. For this example, we'll join on 'Index' to represent a linked transaction.</description>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <join_type>INNER</join_type>
    <step1>Sort Customers</step1>
    <step2>Sort Products</step2>
    <keys_1>
      <key><name>Index</name></key>
    </keys_1>
    <keys_2>
      <key><name>Index</name></key>
    </keys_2>
  </step>
  <step>
    <name>Filter High-Price Products</name>
    <type>FilterRows</type>
    <description>Filters the joined data to include only products with a price greater than $500.</description>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <compare>
      <condition>
        <negated>N</negated>
        <leftvalue>Price</leftvalue>
        <function>&gt;</function>
        <rightvalue>500</rightvalue>
      </condition>
    </compare>
  </step>
  <step>
    <name>Aggregate Sales by Country and Category</name>
    <type>GroupBy</type>
    <description>Groups the data by Country and Category to summarize sales metrics.</description>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <fields>
      <group>
        <field><name>Country</name></field>
        <field><name>Category</name></field>
      </group>
      <aggregate>
        <field><name>Total Revenue</name><subject>Price</subject><type>SUM</type></field>
        <field><name>Customer Count</name><subject>Customer Id</subject><type>COUNT_ANY</type></field>
      </aggregate>
    </fields>
  </step>
  <step>
    <name>Write Sales Report</name>
    <type>TextFileOutput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <separator>,</separator>
    <enclosure>"</enclosure>
    <force_ending_line>N</force_ending_line>
    <header>Y</header>
    <file>
      <name>${OUTPUT_CSV}</name>
      <extension>csv</extension>
      <append>N</append>
      <split>N</split>
      <haspartno>N</haspartno>
      <add_date>N</add_date>
      <add_time>N</add_time>
      <create_parent_folder>Y</create_parent_folder>
      <do_not_create_file_folder>N</do_not_create_file_folder>
      <is_command>N</is_command>
    </file>
    <fields/>
  </step>
  <step_error_handling>
  </step_error_handling>
  <slave-step-copy-partition-distribution>
  </slave-step-copy-partition-distribution>
  <cluster_schema>
    <name>cluster_schema_name</name>
    <dynamic>N</dynamic>
    <steps/>
  </cluster_schema>
  <partitioning_schemas>
  </partitioning_schemas>
  <partition-schema-name>partitions</partition-schema-name>
  <order>
    <hop>
      <from>Read Customers</from>
      <to>Sort Customers</to>
      <enabled>Y</enabled>
    </hop>
    <hop>
      <from>Read Products</from>
      <to>Sort Products</to>
      <enabled>Y</enabled>
    </hop>
    <hop>
      <from>Sort Customers</from>
      <to>Merge Customers and Products</to>
      <enabled>Y</enabled>
    </hop>
    <hop>
      <from>Sort Products</from>
      <to>Merge Customers and Products</to>
      <enabled>Y</enabled>
    </hop>
    <hop>
      <from>Merge Customers and Products</from>
      <to>Filter High-Price Products</to>
      <enabled>Y</enabled>
    </hop>
    <hop>
      <from>Filter High-Price Products</from>
      <to>Aggregate Sales by Country and Category</to>
      <enabled>Y</enabled>
    </hop>
    <hop>
      <from>Aggregate Sales by Country and Category</from>
      <to>Write Sales Report</to>
      <enabled>Y</enabled>
    </hop>
  </order>
</transformation>